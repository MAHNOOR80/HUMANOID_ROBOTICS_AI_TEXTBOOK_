# Environment variables for RAG Agent Backend
# Copy this file to .env and fill in your actual values

# Google API Key (for Gemini LLM)
OPENAI_API_KEY=your_google_api_key_here

# Cohere API Key (for embeddings)
COHERE_API_KEY=your_cohere_api_key_here

# Qdrant Database Configuration
QDRANT_URL=your_qdrant_url_here
QDRANT_API_KEY=your_qdrant_api_key_here

# Optional: Frontend URL for CORS (default: *)
FRONTEND_URL=http://localhost:3000

# Backend API URL (for frontend to connect to backend)
# Development: http://localhost:8000
# Production: https://backend-production-e37e.up.railway.app
API_BASE_URL=http://localhost:8000